
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<main class="ps-main">
    <div class="ps-banner">
        <div class="rev_slider fullscreenbanner"
             style="height: 600px !important"
             id="home-banner">
            <ul>
                <li class="ps-banner"
                    data-index="rs-2972"
                    data-transition="random"
                    data-slotamount="default"
                    data-hideafterloop="0"
                    data-hideslideonmobile="off"
                    data-rotate="0">
                    <img class="rev-slidebg"
                         src="~/images/slider/birthdaybanner.png"
                         alt=""
                         data-bgposition="center center"
                         data-bgfit="cover"
                         data-bgrepeat="no-repeat"
                         data-bgparallax="5"
                         data-no-retina />
                </li>
                <li class="ps-banner ps-banner--white"
                    data-index="rs-100"
                    data-transition="random"
                    data-slotamount="default"
                    data-hideafterloop="0"
                    data-hideslideonmobile="off"
                    data-rotate="0">
                    <img class="rev-slidebg"
                         src="~/images/slider/footerbg2.png"
                         alt=""
                         data-bgposition="center center"
                         data-bgfit="cover"
                         data-bgrepeat="no-repeat"
                         data-bgparallax="5"
                         data-no-retina />
                </li>
                <li class="ps-banner"
                    data-index="rs-297"
                    data-transition="random"
                    data-slotamount="default"
                    data-hideafterloop="0"
                    data-hideslideonmobile="off"
                    data-rotate="0">
                    <img class="rev-slidebg"
                         src="~/images/slider/weddingbg.jpg"
                         alt=""
                         data-bgposition="center center"
                         data-bgfit="cover"
                         data-bgrepeat="no-repeat"
                         data-bgparallax="5"
                         data-no-retina />
                </li>
            </ul>
        </div>
    </div>
    <div class="ps-section--features-product ps-section masonry-root pt-100">
        <div class="ps-container">
            <div class="ps-section__header mb-50">
                <h3 class="ps-section__title" data-mask="Cards">
                    - Categories
                </h3>
            </div>
            <div class="row mb-40">
                <div class="col-md-6">
                    <div class="hoverImage">
                        <a href="~/Home/Birthday">
                            <img src="~/images/birthday/birthdayCategorycard.jpg"
                                 class="img-fluid"
                                 alt="" />
                            <span>BIRTHDAY CARDS</span>
                        </a>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="hoverImage">
                        <a href="~/Home/Wedding">
                            <img src="~/images/wedding/bright-card-cards-color-272243.jpg"
                                 class="img-fluid"
                                 alt="" />

                            <span>WEDDING CARDS</span>
                        </a>
                    </div>
                </div>
            </div>
            <div class="row ">
                <div class="col-md-6">
                    <div class="hoverImage">
                        <a href="~/Home/Newyear">
                            <img src="~/images/newyear/photo-of-2020-on-pink-background-3401900.jpg"
                                 class="img-fluid"
                                 alt="" />

                            <span>NEW YEAR CARDS</span>
                        </a>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="hoverImage">
                        <a href="~/Home/Festival">
                            <img src="~/images/festival/white-and-brown-christmas-gift-box-with-card-726228.jpg"
                                 class="img-fluid"
                                 alt="" />

                            <span>FESTIVAL CARDS</span>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="ps-section ps-section--top-sales ps-owl-root pt-80 pb-80">
        <div class="ps-container">
            <div class="ps-section__header mb-50">
                <div class="row">
                    <div class="col-lg-9 col-md-9 col-sm-12 col-xs-12 ">
                        <h3 class="ps-section__title" data-mask="Cards">
                            - Feature Cards
                        </h3>
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-12 col-xs-12 ">
                        <div class="ps-owl-actions">
                            <a class="ps-prev" href="#"><i class="ps-icon-arrow-right"></i>Prev</a><a class="ps-next" href="#">
                                Next<i class="ps-icon-arrow-left"></i>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="ps-section__content">
                <div class="ps-owl--colection owl-slider"
                     data-owl-auto="true"
                     data-owl-loop="true"
                     data-owl-speed="5000"
                     data-owl-gap="30"
                     data-owl-nav="false"
                     data-owl-dots="false"
                     data-owl-item="4"
                     data-owl-item-xs="1"
                     data-owl-item-sm="2"
                     data-owl-item-md="3"
                     data-owl-item-lg="4"
                     data-owl-duration="1000"
                     data-owl-mousedrag="on">
                    <div class="ps-shoes--carousel">
                        <div class="ps-shoe">
                            <div class="ps-shoe__thumbnail">
                                <img src="~/images/newyear/newyearcard1.png" alt="" /><a class="ps-shoe__overlay"
                                                                                       href="~/Home/Newyear"></a>
                            </div>
                            <div class="ps-shoe__content"></div>
                        </div>
                    </div>
                    <div class="ps-shoes--carousel">
                        <div class="ps-shoe">
                            <div class="ps-shoe__thumbnail">
                                <img src="~/images/birthday/birthdaycard2.png" alt="" /><a class="ps-shoe__overlay"
                                                                                         href="~/Home/Birthday"></a>
                            </div>
                            <div class="ps-shoe__content"></div>
                        </div>
                    </div>
                    <div class="ps-shoes--carousel">
                        <div class="ps-shoe">
                            <div class="ps-shoe__thumbnail">
                                <img src="~/images/festival/festivalcard1.png" alt="" /><a class="ps-shoe__overlay"
                                                                                         href="~/Home/Festival"></a>
                            </div>
                            <div class="ps-shoe__content"></div>
                        </div>
                    </div>
                    <div class="ps-shoes--carousel">
                        <div class="ps-shoe">
                            <div class="ps-shoe__thumbnail">
                                <img src="~/images/wedding/weddingcard.png" alt="" /><a class="ps-shoe__overlay"
                                                                                      href="~/Home/Wedding"></a>
                            </div>
                            <div class="ps-shoe__content"></div>
                        </div>
                    </div>
                    <div class="ps-shoes--carousel">
                        <div class="ps-shoe">
                            <div class="ps-shoe__thumbnail">
                                <img src="~/images/newyear/newyearcard2.png" alt="" /><a class="ps-shoe__overlay"
                                                                                       href="~/Home/Newyear"></a>
                            </div>
                            <div class="ps-shoe__content"></div>
                        </div>
                    </div>
                    <div class="ps-shoes--carousel">
                        <div class="ps-shoe">
                            <div class="ps-shoe__thumbnail">
                                <img src="~/images/festival/festivalcard2.png" alt="" /><a class="ps-shoe__overlay"
                                                                                         href="~/Home/Festival"></a>
                            </div>
                            <div class="ps-shoe__content"></div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</main>